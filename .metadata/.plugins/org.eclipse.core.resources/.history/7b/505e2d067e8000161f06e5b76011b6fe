/**
 * 
 */
package latihan2;
public class DimensionalShapeMain {
	public static void main(String[] args) {
//		default constructor
		DimensionalShape ds = new DimensionalShape();
		
//		parameter constructor 
		DimensionalShape ds2 = new DimensionalShape();
		ds2.setNumSide(4);
		ds2.setRegular(false);
		
//		default constructor 
		System.out.println("Constructor (Default)");
		System.out.println("> jumlah sisi : "+ds.getNumSide());
		System.out.println("> sisinya teratur / tidak : "+ds.isRegular());

//		parameter constructor 
		System.out.println("constructor with params ");
		System.out.println("> jumlah sisi : "+ds2.getNumSide());
		System.out.println("> sisinya teratur / tidak : "+ds2.isRegular());
	}
}